{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "public": true,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/gateway?path=:path*"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/broker/morning-run",
      "schedule": "30 14 * * 1-5"
    }
  ]
}
